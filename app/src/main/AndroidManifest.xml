<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.hurubei.multichoiceexpandablelist">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activities.MultiChoiceExpandableListActivity"
            android:label="MultiChoiceExpandableListActivity"></activity>
        <activity android:name=".activities.MultiChoiceExListActivity"
            android:label="MultiChoiceExListActivity"></activity>
        <activity android:name=".activities.AllSelectExListActivity"
            android:label="AllSelectExListActivity"></activity>
        <activity android:name=".activities.AllSelectListActivity"
            android:label="AllSelectListActivity"></activity>
        <activity android:name=".activities.LongClickMultiChoiceListActivity"
            android:label="LongClickMultiChoiceListActivity"></activity>
    </application>

</manifest>